<idea-plugin>
    <depends>com.intellij.java</depends>
    <extensions defaultExtensionNs="org.jetbrains.iren.language">
        <supporter implementation="org.jetbrains.iren.language.JavaLanguageSupporter"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <renameHandler implementation="org.jetbrains.iren.rename.IRenVariableInplaceRenameHandler"/>
        <renameHandler implementation="org.jetbrains.iren.rename.IRenMemberInplaceRenameHandler"/>
    </extensions>
</idea-plugin>